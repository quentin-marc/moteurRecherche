name : 
    dbo:name
    foaf:name
    rdfs:label
    dbp:name

fondateur :
    dbo:foundedBy > dbr
    dbp:founders > text
    dbp:founder > text

ceo:
    ceo > text

localisation:
    dbp:foundation > text
    (dbp:location > dbr, text)
    dbo:location > dbr
    dbo:locationCity > dbr
    dbo:headquarter > dbr
    dbp:locationCountry > text

année création:
    dbo:foundingYear
    dbo:foundingDate

lien website:
    dbp:homepage
    dbp:website
    (dbo:wikiPageExternalLink > dbr des fois)
    foaf:homepage
    dbo:url


abstract:
select ?abstract where {
OPTIONAL{dbr:Airpush dbo:abstract ?abstract.}
FILTER(langMatches(lang(?abstract), "EN"))
}

name:
select ?name where {
OPTIONAL{dbr:Airpush dbo:name ?name}
OPTIONAL{dbr:Airpush dbp:name ?name}
OPTIONAL{dbr:Airpush rdfs:label ?name}
OPTIONAL{dbr:Airpush foaf:name ?name}
FILTER(langMatches(lang(?name), "EN"))
}

fondateur:
select ?fondateur where {
OPTIONAL{dbr:Microsoft dbo:foundedBy ?fondateur.}
OPTIONAL{dbr:Microsoft dbp:founders ?fondateur.}
OPTIONAL{dbr:Microsoft dbp:founder ?fondateur.}
}

localisation:
select ?localisationLabel where {
OPTIONAL{dbr:Microsoft dbp:foundation ?localisation.
               	?localisation rdfs:label ?localisationLabel.
FILTER(langMatches(lang(?localisationLabel), "EN"))}
OPTIONAL{dbr:Microsoft dbo:location ?localisation.
?localisation rdfs:label ?localisationLabel.
FILTER(langMatches(lang(?localisationLabel), "EN"))}
OPTIONAL{dbr:Microsoft dbo:locationCity ?localisation.
?localisation rdfs:label ?localisationLabel.
FILTER(langMatches(lang(?localisationLabel), "EN"))}
OPTIONAL{dbr:Microsoft dbo:headquarter ?localisation.
?localisation rdfs:label ?localisationLabel.
FILTER(langMatches(lang(?localisationLabel), "EN"))}
OPTIONAL{dbr:Microsoft dbp:locationCountry ?localisation.
?localisation rdfs:label ?localisationLabel.
FILTER(langMatches(lang(?localisationLabel), "EN"))}
}

select str(?foundingYear)  where {
OPTIONAL{dbr:SK_Telecom dbo:foundingDate ?foundingYear.}
OPTIONAL{dbr:SK_Telecom dbo:foundingYear ?foundingYear.}
OPTIONAL{dbr:SK_Telecom dbp:foundation ?foundingYear.}
OPTIONAL{dbr:SK_Telecom dbp:founded ?foundingYear.}
}

select str(?numEmployee) where {
OPTIONAL{dbr:Microsoft dbp:numEmployees ?numEmployee.}
}



